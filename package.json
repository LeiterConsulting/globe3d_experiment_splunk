{
  "name": "how-to-make-react-based-splunk-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:splunk": "node scripts/build-splunk.mjs",
    "build:splunk:v2": "node scripts/build-splunk.mjs --appId splunk_globe_app_v2",
    "splunk:sync": "node scripts/splunk-sync.mjs",
    "splunk:sync:v2": "node scripts/splunk-sync.mjs --appId splunk_globe_app_v2",
    "package:splunk": "node scripts/package-splunk.mjs",
    "package:splunk:v2": "node scripts/package-splunk.mjs --appId splunk_globe_app_v2",
    "branch:v2": "node scripts/branch-v2-app.mjs --appId splunk_globe_app_v2",
    "nasa:q1:smoke:v2": "node scripts/nasa-q1-smoke-v2.mjs",
    "nasa:q1:readiness:v2": "node scripts/nasa-q1-readiness-v2.mjs",
    "demo:ready:v2": "node scripts/demo-ready-v2.mjs",
    "data:expand:hierarchy": "node scripts/expand-hierarchy-lookup.mjs",
    "data:expand:hierarchy:v2": "node scripts/expand-hierarchy-lookup.mjs --appId splunk_globe_app_v2",
    "data:regions:build": "node scripts/generate-region-layer-assets.mjs",
    "data:regions:build:v2": "node scripts/generate-region-layer-assets.mjs --appId splunk_globe_app_v2",
    "data:orbital:build": "node scripts/generate-orbital-datasets.mjs",
    "data:orbital:build:v2": "node scripts/generate-orbital-datasets.mjs --appId splunk_globe_app_v2",
    "template:rename": "node scripts/template-rename.mjs",
